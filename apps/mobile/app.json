      "package": "com.helio.app",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "https://i.ibb.co/bF0fJgS/favicon.png",
        "backgroundColor": "#f1f5f9"
      }
    },
    "web": {
      "favicon": "https://i.ibb.co/bF0fJgS/favicon.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "resizeMode": "contain",
          "backgroundColor": "#f1f5f9"
        }
      ]
    ],
    "owner": "helio-app-owner",
    "updates": {
      "url": "https://u.expo.dev/your-project-uuid-here"
    },
    "runtimeVersion": {
      "policy": "appVersion"
