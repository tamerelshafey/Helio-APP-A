{
  "cli": {
    "version": ">= 9.0.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "APP_VARIANT": "development"
      },
      "projectDir": "apps/mobile"
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "APP_VARIANT": "preview"
      },
      "projectDir": "apps/mobile"
    },
    "production": {
      "android": {
        "buildType": "apk"
      },
      "env": {
        "APP_VARIANT": "production"
      },
      "projectDir": "apps/mobile"
    }
  },
  "submit": {
    "production": {}
  }
}